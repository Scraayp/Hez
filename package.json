{
  "name": "hez",
  "version": "1.0.0",
  "description": "Official Hez discord bot source code.",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "pretty": "prettier --write \"src/**/*.ts\"",
    "compose": "docker-compose up -d",
    "db:generate": "npx prisma generate",
    "db:pull": "npx prisma db pull",
    "db:migrate": "npx prisma migrate dev "
  },
  "main": "src/index.ts",
  "repository": "https://github.com/HostSapling/Hostsapling-Bot.git",
  "author": "Scraayp (Michal) <scraayp@hostsapling.net> | Hostsapling Team",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "@sentry/node": "^7.61.0",
    "chalk": "4.1.2",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "phisherman.gg": "^0.0.2"
  },
  "devDependencies": {
    "eslint": "^8.35.0",
    "prettier": "^3.0.1",
    "prisma": "^4.11.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
